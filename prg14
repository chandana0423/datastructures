#include<stdio.h>
#include<conio.h>
#include<string.h>
int main()
{
 int t,n,i,j,flag=0;
 scanf("%d",&t);
 while(t--)
 {
  int n;
  char name[90];
  scanf("%s",name);
  n=strlen(name);
  for(i=0,j=n-1;i<=(n-1/2);i++,j--)
  {
  	if(name[i]!=name[j])
  	{
  		flag=1;
  		break;
	  } 
  }
   if(flag==1)
     printf("NO");
    else
     printf("yes");
}
}
